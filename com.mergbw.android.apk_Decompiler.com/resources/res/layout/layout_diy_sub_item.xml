<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginStart="5dp">
    <FrameLayout android:background="@drawable/bg_group_item_solid" android:layout_width="68dp" android:layout_height="68dp" android:layout_margin="5dp">
        <com.mergbw.core.ui.views.SubItemListView android:gravity="center" android:id="@+id/view_sub_list" android:layout_width="match_parent" android:layout_height="match_parent"/>
        <ImageView android:layout_gravity="center" android:id="@+id/iv_add" android:visibility="gone" android:layout_width="44dp" android:layout_height="44dp" android:src="@mipmap/icon_add"/>
    </FrameLayout>
    <ImageView android:layout_gravity="end" android:id="@+id/iv_delete" android:visibility="gone" android:layout_width="25dp" android:layout_height="25dp" android:src="@mipmap/icon_delete_02"/>
</FrameLayout>
